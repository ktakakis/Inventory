@model netcore.Models.Invent.MoneyTransferOrder

@{
    ViewData["Title"] = "Λεπτομέρειες";
    Layout = "~/Views/Shared/_AdminlteNetcoreDetails.cshtml";
    ViewData["EntityIcon"] = "fa fa-exchange";
    ViewData["DetailInfo"] = "Εάν η διαδικασία μεταφοράς τρέχει ήδη (αποστολή και παραλαβή προϊόντων ), τα προϊόντα δεν μπορούν να υποστούν επεξεργασία, διαγραφή ή προσθήκη.";
}

    <div class="row">
        <div class="col-md-9">
            <!-- Default box -->
            <div class="box box-primary">
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <input type="hidden" asp-for="MoneyTransferOrderId" />
                            <input type="hidden" asp-for="MoneyTransferOrderNumber" />
                            <input type="hidden" asp-for="MoneyTransferOrderStatus" />
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="MoneyTransferOrderNumber" class="control-label"></label>
                                        <input asp-for="MoneyTransferOrderNumber" class="form-control" disabled />
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="MoneyTransferOrderDate" class="control-label"></label>
                                        <div class="input-group date  time">
                                            <div class="input-group-addon">
                                                <i class="fa fa-calendar"></i>
                                            </div>
                                            <input disabled  asp-for="MoneyTransferOrderDate" value="@Model.MoneyTransferOrderDate.ToString("dd/MM/yyyy")" class="form-control pull-right datepicker" lang="el" type="text" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label asp-for="Description" class="control-label"></label>
                                        <textarea  disabled asp-for="Description" class="form-control" rows="3"></textarea>
                                        <span asp-validation-for="Description" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="CashRepositoryIdFrom" class="control-label">Από Ταμείο</label>
                                        <select disabled  asp-for="CashRepositoryIdFrom" id="CashRepositoryIdFrom" class="form-control select2" asp-items="ViewBag.CashRepositoryIdFrom"></select>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="CashRepositoryIdTo" class="control-label">Στο Ταμείο</label>
                                        <select disabled  asp-for="CashRepositoryIdTo" id="CashRepositoryIdTo" class="form-control select2" asp-items="ViewBag.CashRepositoryIdTo"></select>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="PaymentAmount" class="control-label"></label>
                                        <input disabled  asp-for="PaymentAmount" class="form-control" />
                                        <span asp-validation-for="PaymentAmount" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="MoneyTransferOrderStatus" class="control-label"></label>
                                        <select disabled  asp-for="MoneyTransferOrderStatus" class="form-control select2" asp-items="Html.GetEnumSelectList<netcore.Models.Invent.MoneyTransferOrderStatus>()" ></select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.box-body -->
                <div class="box-footer">
                    <div class="form-group">
                        <a asp-action="Edit" asp-route-id="@Model.MoneyTransferOrderId" class="btn btn-primary">Επεξεργασία</a>
                        <a asp-action="Index" class="btn btn-default">Επιστροφή στη λίστα</a>
                    </div>
                </div>
                <!-- /.box-footer-->
            </div>
        </div>
        <div class="col-md-3">
            @await Html.PartialAsync("~/Views/Shared/_AdminlteNetcoreTimeline.cshtml")
        </div>
    </div>