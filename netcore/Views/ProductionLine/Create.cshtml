@model netcore.Models.Invent.ProductionLine

@{
    ViewData["Title"] = "Δημιουργία";
    Layout = "~/Views/Shared/_AdminlteNetcorePopup.cshtml";
}
<div class="row">
    <div class="col-md-4">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="ProductionId" class="control-label"></label>
            <select asp-for="ProductionId" class="form-control" asp-items="ViewBag.ProductionId"></select>
        </div>
        <div class="form-group">
            <label asp-for="branchId" class="control-label"></label>
            <select asp-for="branchId" class="form-control" asp-items="ViewBag.branchId"></select>
        </div>
        <div class="form-group">
            <label asp-for="warehouseId" class="control-label"></label>
            <select asp-for="warehouseId" class="form-control" asp-items="ViewBag.warehouseId"></select>
        </div>
        <div class="form-group">
            <label asp-for="productId" class="control-label"></label>
            <select asp-for="productId" class="form-control" asp-items="ViewBag.productId"></select>
        </div>
        <div class="form-group">
            <label asp-for="qty" class="control-label"></label>
            <input asp-for="qty" class="form-control" />
            <span asp-validation-for="qty" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
