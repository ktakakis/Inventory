@model IEnumerable<netcore.Models.Invent.ProductionLine>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminlteNetcoreIndex.cshtml";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.qty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.createdAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Production)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.branch)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.product)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.warehouse)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.qty)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.createdAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Production.ProductionId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.branch.branchId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.product.productId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.warehouse.warehouseId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ProductionLineId"><i class="fa fa-pencil"></i></a> |
                <a asp-action="Details" asp-route-id="@item.ProductionLineId"><i class="fa fa-list"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.ProductionLineId"><i class="fa fa-trash"></i></a>
            </td>
        </tr>
        }
    </tbody>
</table>
