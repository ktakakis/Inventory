@model netcore.Models.Invent.CashflowOut

@{
    ViewData["Title"] = "Επεξεργασία";
    Layout = "~/Views/Shared/_AdminlteNetcoreEdit.cshtml";
    ViewData["EntityIcon"] = "fa fa-exchange";
    ViewData["EditInfo"] = " Η Εντολή Μεταφοράς Χρημάτων έχει 3 καταστάσεις: 1. Αίτηση (Μη επιβεβαιωμένη, δεν είναι έτοιμη για μεταφορά), 2. Έγκριση (Επιβεβαιωμένη, έτοιμη για μεταφορά) και 3. Ολοκληρωμένη (Κλειστή, έχει ήδη γίνει η διαδικασία μεταφοράς).";
}

@Html.Partial("_StatusMessage", (string)ViewBag.StatusMessage)
<div class="row">
    <div class="col-md-4">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="CashflowOutId" />
        <div class="form-group">
            <label asp-for="MoneyTransferOrderId" class="control-label"></label>
            <select asp-for="MoneyTransferOrderId" class="form-control" asp-items="ViewBag.MoneyTransferOrderId"></select>
            <span asp-validation-for="MoneyTransferOrderId" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="CashflowOutNumber" class="control-label"></label>
            <input asp-for="CashflowOutNumber" class="form-control" />
            <span asp-validation-for="CashflowOutNumber" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="CashflowOutDate" class="control-label"></label>
            <input asp-for="CashflowOutDate" class="form-control" />
            <span asp-validation-for="CashflowOutDate" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Description" class="control-label"></label>
            <input asp-for="Description" class="form-control" />
            <span asp-validation-for="Description" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="CashRepositoryIdFrom" class="control-label"></label>
            <input asp-for="CashRepositoryIdFrom" class="form-control" />
            <span asp-validation-for="CashRepositoryIdFrom" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="CashRepositoryIdTo" class="control-label"></label>
            <input asp-for="CashRepositoryIdTo" class="form-control" />
            <span asp-validation-for="CashRepositoryIdTo" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="HasChild" class="control-label"></label>
            <input asp-for="HasChild" class="form-control" />
            <span asp-validation-for="HasChild" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="createdAt" class="control-label"></label>
            <input asp-for="createdAt" class="form-control" />
            <span asp-validation-for="createdAt" class="text-danger"></span>
        </div>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
